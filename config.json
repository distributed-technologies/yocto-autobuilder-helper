        "buildtools" : {
            "MACHINE" : "qemux86-64",
            "step1" : {
                "SDKMACHINE" : "x86_64",
                "BBTARGETS" : "buildtools-tarball uninative-tarball"
            },
            "step2" : {
                "SDKMACHINE" : "i686",
                "BBTARGETS" : "uninative-tarball"
